set sandbox none
set statusbar-h-padding 0
set statusbar-v-padding 0
set page-padding 1
set selection-clipboard clipboard
map u scroll half-up
map d scroll half-down
map D toggle_page_mode
map r reload
map R rotate
map K zoom in
map J zoom out
map i recolor
map p print

set default-bg                     "#0a0a08"
set default-fg                     "#dfdfdf"
set statusbar-bg                   "#0a0a08"
set statusbar-fg                   "#dfdfdf"
set inputbar-bg                    "#0a0a08"
set inputbar-fg                    "#dfdfdf"
